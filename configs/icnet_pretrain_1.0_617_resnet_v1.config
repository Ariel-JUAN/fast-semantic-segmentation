# Fine-tuned ICNet with Resnet-50 from PSPNet (v1), Atrous version

model {
    icnet {
        num_classes: 19
        filter_scale: 1.0
        feature_extractor {
            type: 'dilated_resnet50'
        }
        hyperparams {
            batch_norm {
                train: true
                scale: true
                center: true
                decay: 0.9997
                epsilon: 0.00001
            }
            regularizer {
                l2_regularizer {
                     weight: 0.0001
                }
            }
            initializer {
                variance_scaling_initializer {}
            }
        }
        loss {
            classification_loss {
                softmax {}
            }
            use_auxiliary_loss: true
        }
        pretrain_single_branch_mode: true
    }
}

train_config: {
    batch_size: 18
    num_steps: 90000
    fine_tune_checkpoint_type: "classification"
    fine_tune_checkpoint: "tmp/resnet50/resnet_v1_50.ckpt"
    optimizer {
        momentum_optimizer: {
            learning_rate: {
                polynomial_decay_learning_rate {
                    initial_learning_rate: 0.01
                    decay_steps: 90000
                    power: 0.9
                }
            }
            momentum_optimizer_value: 0.9
        }
    }
    preprocessor_step {
        random_image_scale {
            min_scale_ratio: 0.7
            max_scale_ratio: 2.0
        }
    }
    preprocessor_step {
        random_image_crop {
            crop_height: 617
            crop_width: 617
            images_channel_dim: 3
            labels_channel_dim: 1
        }
    }
    preprocessor_step {
        random_horizontal_flip {}
    }
    batch_queue_capacity: 150
    num_batch_queue_threads: 35
    prefetch_queue_capacity: 10
}

train_input_reader: {
    num_examples: 2975
    shuffle: true
    tf_record_input_reader {
        input_path: "tmp/cityscapes_train.record"
    }
    num_readers: 25
}


eval_config: {
    num_examples: 500
    crop_height: 1025
    crop_width: 2049
}

eval_input_reader: {
    shuffle: false
    tf_record_input_reader {
        input_path: "tmp/cityscapes_val.record"
    }
    num_readers: 1
}
